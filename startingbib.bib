	note = {ADS Bibcode: 2012SPIE.8443E..1AD},
	pages = {84431A},
	file = {Full Text PDF:/home/zcfa/Zotero/storage/C6F4AHTK/Davis et al. - 2012 - Raytracing with MARX x-ray observatory design, calibration, and support.pdf:application/pdf},
}

