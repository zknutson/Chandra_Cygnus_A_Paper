	note = {Publisher: IOP
ADS Bibcode: 2008ApJ...685..828I},
	keywords = {X-rays: galaxies, Astrophysics, galaxies: individual: Cygnus A 3C 223 3C 284 3C 219, radiation mechanisms: non-thermal, radio continuum: galaxies},
	pages = {828--838},
	file = {Full Text PDF:/home/zcfa/Zotero/storage/RJBD5ZHX/Ito et al. - 2008 - The Estimate of Kinetic Power of Jets in FR II Radio Galaxies Existence of Invisible Components.pdf:application/pdf},
