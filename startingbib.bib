	pages = {L37--L40},
	file = {Full Text PDF:/home/zcfa/Zotero/storage/2GR36L8C/Ogle et al. - 1997 - Scattered Nuclear Continuum and Broad Hα in Cygnus A.pdf:application/pdf},
